{
  "name": "Activity",
  "plural": "Activities",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "date_and_time": {
      "type": "date",
      "required": true
    },
    "user_count": {
      "type": "number",
      "required": true
    },
    "about": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "customer_id"
    },
    "ActivityCustomers": {
      "type": "hasMany",
      "model": "Customer",
      "foreignKey": "activity_id",
      "through": "hasActivity"
    },
    "activitySubcategory": {
      "type": "belongsTo",
      "model": "ActivitySubcategory",
      "foreignKey": "subcategory_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "find"
    }
  ],
  "methods": {}
}
